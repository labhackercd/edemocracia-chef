[settings]

EMAIL_HOST = <%= node['config']['wikilegis']['email_host'] %>
EMAIL_HOST_USER = <%= node['config']['wikilegis']['email_address'] %>
EMAIL_HOST_PASSWORD = <%= node['config']['wikilegis']['email_password'] %>
EMAIL_PORT = <%= node['config']['wikilegis']['email_port'] %>
EMAIL_USE_TLS = <%= node['config']['wikilegis']['email_use_tls'] %>
DEFAULT_FROM_EMAIL = <%= node['config']['wikilegis']['email_from'] %>
DJANGO_SECRET_KEY = <%= File.read("#{node['config']['wikilegis']['dir']}/secret.key").strip %>
API_KEY = <%= File.read("#{node['config']['wikilegis']['dir']}/api.key").strip %>
DEBUG = False
DATABASE_URL = postgres://wikilegis@127.0.0.1:5432/wikilegis
COMPRESS_OFFLINE = True
FORCE_SCRIPT_NAME = <%= node['config']['wikilegis']['url_prefix'] %>
STATIC_URL = <%= node['config']['wikilegis']['url_prefix'] %>/static/
MEDIA_URL = <%= node['config']['wikilegis']['url_prefix'] %>/media/
LOGIN_URL = <%= node['config']['wikilegis']['login_url'] %>