[settings]

EMAIL_HOST = <%= node['config']['audiencias']['email_host'] %>
EMAIL_HOST_USER = <%= node['config']['audiencias']['email_address'] %>
EMAIL_HOST_PASSWORD = <%= node['config']['audiencias']['email_password'] %>
EMAIL_PORT = <%= node['config']['audiencias']['email_port'] %>
EMAIL_USE_TLS = <%= node['config']['audiencias']['email_use_tls'] %>
